<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN"
		"http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="hr.crosig.proposal">
	<namespace>AP_Proposal</namespace>
	<entity local-service="true" name="CoveragePlan" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="coveragePlanId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="description" type="String" />
		<column name="category" type="String" />

		<!-- Finders -->

		<finder name="Name" return-type="CoveragePlan">
			<finder-column name="name" />
		</finder>

		<finder name="Category" return-type="Collection">
			<finder-column name="category" />
		</finder>
	</entity>
	<entity local-service="true" name="InsuredRole" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="InsuredRoleId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="title" type="String" />
		<column name="name" type="String" />
		<column name="externalId" type="String" />

		<finder name="InsuredRoleId" return-type="InsuredRole">
			<finder-column name="InsuredRoleId" />
		</finder>
	</entity>
	<entity local-service="true" name="PolicyCoverageOpt" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="policyCoverageOptionId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="proposalId" type="long" />
		<column name="coverageOptionsName" type="String" />
		<column name="coverageOptionsValue" type="String" />
		<column name="type" type="String" />

		<finder name="ProposalId" return-type="Collection">
			<finder-column name="proposalId" />
		</finder>
	</entity>
	<entity local-service="true" name="PolicyOptions" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="policyOptionsId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="proposalId" type="long" />
		<column name="currency" type="String" />
		<column name="termsDate" type="Date" />
		<column name="productCategory" type="String" />
		<column name="productExtNumber" type="String" />
		<column name="issueDate" type="Date" />
		<column name="contractStartDate" type="Date" />
		<column name="contractEndDate" type="Date" />
		<column name="contractPeriod" type="String" />
		<column name="durationYear" type="int" />
		<column name="policyStartDate" type="Date" />
		<column name="policyEndDate" type="Date" />
		<column name="policyNumberDays" type="int" />
		<column name="communicationMethod" type="String" />

		<finder name="ProposalId" return-type="PolicyOptions" unique="true">
			<finder-column name="proposalId" />
		</finder>
	</entity>
	<entity local-service="true" name="Product" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="productId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="externalId" type="long" />
		<column name="active" type="boolean" />
		<column name="description" type="String" />
		<column name="category" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="name" />
		</order>

		<!-- Finder -->

		<finder name="Name" return-type="Product">
			<finder-column name="name" />
		</finder>
	</entity>
	<entity local-service="true" name="ProductRole" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="productRoleId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="productId" type="long" />
		<column name="roleId" type="long" />

		<finder name="RoleId" return-type="Collection">
			<finder-column name="roleId" />
		</finder>
	</entity>
	<entity local-service="true" name="Proposal" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="proposalId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="externalProposalNumber" type="String" />
		<column name="lastUpdate" type="Date" />
		<column name="origin" type="String" />
		<column name="agentUserId" type="long" />
		<column name="policyHolderExtNumber" type="String" />
		<column name="insuredObjectExtNumber" type="String" />
		<column name="status" type="String" />

		<finder name="AgentUserId" return-type="Collection">
			<finder-column name="agentUserId"></finder-column>
		</finder>
	</entity>
	<entity local-service="true" name="ProposalContact" remote-service="false" uuid="false">

		<!-- PK fields -->

		<column name="proposalContactId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="proposalId" type="long" />
		<column name="contactExtNumber" type="String" />
		<column name="insuredRoles" type="String" />

		<finder name="ProposalId" return-type="Collection">
			<finder-column name="proposalId" />
		</finder>
	</entity>
</service-builder>